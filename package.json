{
  "name": "electron_react_parcel_tsnode",
  "version": "0.0.1",
  "main": "./main.ts",
  "license": "MIT",
  "scripts": {
    "web": "parcel src/client/index.html",
    "start": "node ./scripts/dev.js",
    "build": "parcel build ./src/client/index.html -d ./build --public-url ./ --no-cache",
    "tse": "cross-env NODE_ENV=development electron -r ts-node/register ./src/desktop/main.ts",
    "rebuild": "electron-rebuild",
    "force:rebuild": "npm rebuild --runtime=electron --target=4.0.0 --disturl=https://atom.io/download/atom-shell --abi=64",
    "prod": "cross-env NODE_ENV=production npm-run-all build",
    "test": "jest"
  },
  "dependencies": {
    "bulma": "0.7.2",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "tslib": "1.9.3",
    "when-switch": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-decorators": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.2.0",
    "@babel/plugin-transform-typescript": "7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "7.2.0",
    "@types/jest": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "babel-plugin-module-resolver": "^3.1.1",
    "electron": "4.0.0",
    "electron-rebuild": "1.8.2",
    "gulp": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "jest": "latest",
    "jsdom": "latest",
    "less": "3.8.1",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.11.0",
    "postcss": "7.0.5",
    "postcss-csso": "latest",
    "postcss-easy-import": "latest",
    "postcss-preset-env": "latest",
    "postcss-svgo": "4.0.0",
